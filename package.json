{
  "name": "tosho",
  "version": "1.0.0",
  "description": "Your personal librarian.",
  "main": "./dist/main.js",
  "scripts": {
    "pretty": "npx prettier --write .",
    "eslint": "eslint --fix ./src && eslint --fix ./configs",
    "start": "electron .",
    "build:main": "NODE_ENV=development webpack --config webpack.preload.js --mode development && NODE_ENV=development webpack --config webpack.main.js --mode development",
    "build:rend": "NODE_ENV=development webpack --config webpack.renderer.js --mode development",
    "build": "rm -rf ./dist && NODE_ENV=development webpack --config webpack.preload.js --mode development && NODE_ENV=development webpack --config webpack.main.js --mode development && NODE_ENV=development webpack --config webpack.renderer.js --mode development",
    "main": "NODE_ENV=development webpack --config webpack.preload.js --mode development && NODE_ENV=development webpack --config webpack.main.js --mode development && electron .",
    "rend": "NODE_ENV=development webpack-dev-server --config webpack.renderer.js --mode development"
  },
  "keywords": [
    "library",
    "book",
    "organizer",
    "desktop",
    "modern",
    "minimalist"
  ],
  "author": "Leonel Ramos",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react": "^6.24.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "prettier": "2.4.1",
    "webpack": "^5.55.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.8",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^4.1.17",
    "pdfjs": "^2.4.6",
    "react": "^17.0.2",
    "react-burger-menu": "^3.0.6",
    "react-dom": "^17.0.2",
    "electron": "^15.1.0"
  }
}
