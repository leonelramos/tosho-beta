{
  "name": "tosho",
  "version": "1.0.0",
  "description": "Your personal librarian.",
  "main": "./dist/main.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "prettier": "npx prettier --write .",
    "eslint": "eslint --fix ./src ./webpack/webpack.*.js",
    "start": "electron .",
    "build:main": "cross-env NODE_ENV=development webpack --config webpack/webpack.preload.js --mode development && cross-env NODE_ENV=development webpack --config webpack/webpack.main.js --mode development",
    "build:rend": "cross-env NODE_ENV=development webpack --config webpack/webpack.renderer.js --mode development",
    "build": "cross-env NODE_ENV=development webpack --config webpack/webpack.preload.js --mode development && cross-env NODE_ENV=development webpack --config webpack/webpack.main.js --mode development && cross-env NODE_ENV=development webpack --config webpack/webpack.renderer.js --mode development",
    "build:test": "cross-env NODE_ENV=production webpack --config webpack/webpack.preload.js --mode production && cross-env NODE_ENV=production webpack --config webpack/webpack.main.js --mode production && cross-env NODE_ENV=production webpack --config webpack/webpack.renderer.js --mode production",
    "main": "cross-env NODE_ENV=development webpack --config webpack/webpack.preload.js --mode development && cross-env NODE_ENV=development webpack --config webpack/webpack.main.js --mode development && electron .",
    "rend": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.renderer.js --mode development"
  },
  "keywords": [
    "library",
    "book",
    "ebooks",
    "organizer",
    "desktop",
    "modern",
    "minimalist"
  ],
  "author": "Leonel Ramos",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/lodash": "^4.14.175",
    "@types/node": "^14.17.20",
    "@types/pdfjs-dist": "^2.7.5",
    "@types/prettier": "^2.4.1",
    "@types/react": "^17.0.27",
    "@types/react-burger-menu": "^2.8.1",
    "@types/react-dom": "^17.0.9",
    "@types/webpack": "^4.41.31",
    "@types/webpack-dev-server": "^4.3.0",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.3",
    "webpack": "^5.55.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.8",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "electron": "^15.1.0",
    "epubjs": "^0.3.88",
    "framer-motion": "^4.1.17",
    "html-webpack-plugin": "^5.3.2",
    "jszip": "^3.7.1",
    "lodash": "^4.17.21",
    "lowdb": "^3.0.0",
    "pdfjs": "^2.4.6",
    "react": "^17.0.2",
    "react-burger-menu": "^3.0.6",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "xmldom": "^0.6.0"
  }
}
