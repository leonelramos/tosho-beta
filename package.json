{
  "name": "tosho",
  "version": "1.0.0",
  "description": "Your personal librarian.",
  "main": "./dist/electron/main.js",
  "scripts": {
    "run" : "electron .",
    "build:main": "NODE_ENV=development webpack --config webpack.config.main.js --mode development",
    "build:rend": "NODE_ENV=development webpack --config webpack.config.renderer.js --mode development",
    "build:all": "NODE_ENV=development webpack --config webpack.config.main.js --mode development && NODE_ENV=development webpack --config webpack.config.renderer.js --mode development",
    "run:main": "NODE_ENV=development webpack --config webpack.config.main.js --mode development && electron .",
    "serve:rend": "NODE_ENV=development webpack-dev-server --config webpack.config.renderer.js --mode development"
  },
  "keywords": [
    "library",
    "book",
    "organizer",
    "desktop",
    "modern",
    "minimalist"
  ],
  "author": "Leonel Ramos",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react": "^6.24.1",
    "electron": "^15.0.0",
    "html-webpack-plugin": "^5.3.2",
    "webpack": "^5.55.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
